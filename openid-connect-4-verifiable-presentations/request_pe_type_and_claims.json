{
    "id_token": {
        "email":null,
        "verifiable_presentations": {
            "comment": "verifiable_presentations element is a presentation_definition",
            "input_descriptors": [
                {
                    "schema": {
                        "uri": [
                            "https://www.w3.org/2018/credentials/examples/v1/IDCardCredential"
                        ]
                    },
                    "constraints": {
                        "format": {
                            "comment": "extension to PE syntax - vc only - use path instead",
                            "jwt_vc": {
                               "alg": [
                                  "ES256K",
                                  "ES384"
                               ]
                            }
                        },
                        "limit_disclosure": "required",
                        "fields": [
                            {
                                "path": [
                                    "$.vc.credentialSubject.given_name"
                                ]
                            },
                            {
                                "path": [
                                    "$.vc.credentialSubject.family_name"
                                ]
                            },
                            {
                                "path": [
                                    "$.vc.credentialSubject.birthdate"
                                ]
                            }
                        ]
                    }
                }
            ]
        }
    }
}